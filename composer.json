{
  "name": "web-fu/mapper",
  "type": "library",
  "description": "Map anything into anything",
  "license": "MIT",
  "require": {
    "php": "8.0.* || 8.1.* || 8.2.*"
  },
  "autoload": {
    "psr-4": {
      "WebFu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebFu\\Tests\\": "tests/"
    },
    "files": [
      "src/bootstrap.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "friendsofphp/php-cs-fixer": "^3.13",
    "phpstan/phpstan": "^1.9"
  }
}
